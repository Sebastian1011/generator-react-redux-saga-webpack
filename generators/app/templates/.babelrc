{
  "presets": [
    "latest",
    "react",
    "stage-0",
    "es2017"
  ],
  "plugins": [
    [
      "babel-plugin-filenamespace",
      {
        "root": "src/actions",
        "separator": "_"
      }
    ],
    //antd使用babel-plugin-import加载用到的组件，只需引入模块，无需单独引入样式
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}
