{
    "name": "generator-react-redux-saga-webpack",
    "version": "0.0.0",
    "description": "Yeoman generator fro using react with redux, webpack and saga via Babel",
    "homepage": "https://github.com/Sebastian1011/generator-react-redux-saga-webpack",
    "author": {
        "name": "zmzhang",
        "email": "zzmbpt@163.com",
        "url": "https://github.com/Sebastian1011"
    },
    "files": [
        "generators"
    ],
    "main": "generators/index.js",
    "keywords": [
        "react",
        "redux",
        "saga",
        "webpack",
        "babel",
        "generator",
        "yeoman-generator"
    ],
    "devDependencies": {
        "yeoman-test": "^1.7.0",
        "yeoman-assert": "^3.1.0",
        "coveralls": "^2.13.1",
        "nsp": "^2.6.3",
        "eslint": "^4.1.0",
        "prettier": "^1.7.0",
        "husky": "^0.14.3",
        "lint-staged": "^4.2.1",
        "eslint-config-prettier": "^2.4.0",
        "eslint-plugin-prettier": "^2.2.0",
        "eslint-config-xo": "^0.18.2",
        "jest": "^21.0.0",
        "jest-cli": "^21.0.0"
    },
    "dependencies": {
        "yeoman-generator": "^2.0.1",
        "chalk": "^2.1.0",
        "yosay": "^2.0.1"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "scripts": {
        "prepublish": "nsp check",
        "pretest": "eslint .",
        "precommit": "lint-staged",
        "test": "jest"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "env": {
            "jest": true,
            "node": true
        },
        "extends": [
            "xo",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error",
                {
                    "printWidth": 90,
                    "singleQuote": true
                }
            ]
        }
    },
    "repository": "Sebastian1011/generator-react-redux-saga-webpack",
    "license": "MIT"
}
